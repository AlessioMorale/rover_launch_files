mkdir -p ~/startup_scripts/
cp ./startup_launch ~/startup_scripts/ 
sed -i 's|\*roverws\*|'"`pwd`/../../.."'|g' ~/startup_scripts/startup_launch
sed -i 's|\*rosws\*|'"`rospack find rosbash`/../.."'|g' ~/startup_scripts/startup_launch

sudo cp ./ros_rover.service /lib/systemd/system/
sudo sed -i 's|\*startup_scripts\*|'`echo ~/startup_scripts`'|g' /lib/systemd/system/ros_rover.service
sudo systemctl daemon-reload
sudo systemctl enable ros_rover.service
sudo systemctl start ros_rover.service
sudo systemctl status ros_rover.service
