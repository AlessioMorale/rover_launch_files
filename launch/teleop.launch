<?xml version="1.0"?>
<launch>
  <!-- params -->
  <rosparam command="load" file="$(find rover_launch_files)/config/rover.yaml" /> -->

  <!-- nodes -->
  <node name="joy_node" pkg="joy" type="joy_node" respawn="true" />
  <node name="joy_teleop" pkg="joy_teleop" type="joy_teleop.py" />
  <node pkg="twist_mux" type="twist_mux" name="twist_mux">
      <rosparam command="load" file="$(find rover_launch_files)/config/twist_mux.yaml" />
      <remap from="cmd_vel_out" to="velocity_controller/cmd_vel"/>
  </node>
 </launch>